:root {
    --bs-blue: #0d6efd;
    --bs-indigo: #6610f2;
    --bs-purple: #6f42c1;
    --bs-pink: #d63384;
    --bs-red: #dc3545;
    --bs-orange: #fd7e14;
    --bs-yellow: #ffc107;
    --bs-green: #198754;
    --bs-teal: #20c997;
    --bs-cyan: #0dcaf0;
    --bs-white: #fff;
    --bs-gray: #6c757d;
    --bs-gray-dark: #343a40;
    --bs-gray-100: #f8f9fa;
    --bs-gray-200: #e9ecef;
    --bs-gray-300: #dee2e6;
    --bs-gray-400: #ced4da;
    --bs-gray-500: #adb5bd;
    --bs-gray-600: #6c757d;
    --bs-gray-700: #495057;
    --bs-gray-800: #343a40;
    --bs-gray-900: #212529;
    --bs-primary: #0d6efd;
    --bs-secondary: #6c757d;
    --bs-success: #198754;
    --bs-info: #0dcaf0;
    --bs-warning: #ffc107;
    --bs-danger: #dc3545;
    --bs-light: #f8f9fa;
    --bs-dark: #212529;
    }
html{
			scroll-behavior: smooth;
}
*{
			font-family:'Poppins',san-serif ;
			box-sizing: border-box;
			padding: 0;
			margin: 0;
			color: white;
			}
.navbar-brand{
			font-weight: 400;
}
.nav-item .nav-link,
.nav-item active .nav-link{
			font-family: 'Fira Sans',san-serif;
}
.navbar-brand img{
			font-family: 'Dancing Script'san-serif;
}

body {
		background-image: var(--bs-gradient);.	
}
#grad {
  background-image: linear-gradient(to bottom right, #545454, #363636);
  height: 100vh;
  position: relative;
  align-content: center;
  justify-content: center;
  align-items: center;
}
#grad h1{
		 text-align: center;
		 padding: 55px;
		 color: whitesmoke;
		 font-family: 'Fira Sans',san-serif;
		 
}
#grad h2{
    color: whitesmoke;
    padding: 55px;
    text-align: center;
    background-image: linear-gradient(to bottom right, #545454, #363636);
  border-collapse: none;
}
#grad #about{
	background-image: linear-gradient(to bottom right, #545454, #363636);		
	text-align: center;
	align-items: center;
	}
#grad img{
			margin: 50px;
}
#grad #about p{
			color: #fff;
}
#docs h1{
font-size: 80px;
			margin: 20px;
			padding:20px ;
			border: 6px dashed whitesmoke;
			font-family: 'Dongle',san-serif;
}
.a-a-home{
			margin-left:160px ;
